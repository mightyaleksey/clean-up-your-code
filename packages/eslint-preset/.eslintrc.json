{
  "env": {
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "arrow-body-style": [2, "as-needed"],
    "arrow-parens": [2, "as-needed", {"requireForBlockBody": true}],
    "arrow-spacing": 2,
    "block-spacing": [2, "always"],
    "brace-style": [2, "1tbs"],
    "capitalized-comments": 0,
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": [0, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "curly": [2, "multi"],
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": [2, "always"],
    "eqeqeq": [2, "always"],
    "func-call-spacing": [2, "never"],
    "generator-star-spacing": [2, {"before": true, "after": false}],
    "indent": [2, 2],
    "jsx-quotes": [2, "prefer-single"],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true, "mode": "minimum", "align": "value"}],
    "keyword-spacing": [2, {"before": true, "after": true}],
    "linebreak-style": [2, "unix"],
    "no-var": 2,
    "object-curly-newline": [2, {"minProperties": 2, "multiline": true}],
    "object-curly-spacing": [2, "never"],
    "one-var": [2, "never"],
    "prefer-const": 2,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, {"nonwords": false, "words": true}],
    "strict": 2
  }
}
